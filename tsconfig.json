{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "src",
    "paths": {
      "@pages/*": ["pages/*"],
      "@libs/*": ["libs/*"],
      "@static/*": ["static/*"],
      "@ui/layouts/*": ["ui/layouts/*"],
      "@ui/components/*": ["ui/components/*"],
      "@ui/blocks/*": ["ui/blocks/*"],
      "@astro.config.mjs": ["../astro.config.mjs"]
    },
    "types": []
  }
}
